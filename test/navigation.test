UTest({
    async 'navigate' () {
        let url = 'file://' + __dirname + '/html/a.html';
        console.log(url);
        let q = await Lib.SQuery.load(url);
        let anchor = await q.find('a').click();
        
        let next = await q.waitForPageLoad();
        let heading = await next.find('h1').text();
        eq_(heading, 'Foo');
    }
})