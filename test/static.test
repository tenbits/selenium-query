UTest({
	'load local file' (done) {
		Lib
			.SQuery
			.load('/test/html/foo.html')
			.find('.foo')
			.done($ => {
				eq_($.length, 2)
				done();
			});
	},
	'load google' (done) {
		Lib
			.SQuery
			.load('http://google.com')
			.find('input')
			.css('background-color', 'red')
			.done($ => {
				notEq_($.length, 0)
				done();
			});
	}
})

