{
    "name": "selenium-query",
    "description": "jQuery API for Selenium WebDriver/JSDom",
    "version": "0.52.48",
    "contributors": [],
    "main": "./lib/query.js",
    "types": "./lib/query.d.ts",
    "author": "Alexander Kit <alex.kit@atmajs.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/tenbits/selenium-query.git"
    },
    "bugs": {
        "url": "https://github.com/atmajs/selenium-query/issues"
    },
    "licenses": [
        "MIT"
    ],
    "devDependencies": {
        "@types/cheerio": "^0.22.10",
        "@types/node": "^16.10.1",
        "app-bundler": "^0.1.2",
        "atma": "^0.15.6",
        "atma-io-middleware-importer": "^1.1.30",
        "atma-loader-babel": "^1.0.19",
        "atma-loader-ts": "^1.1.17",
        "atma-server": "^0.5.12",
        "dts-bundle": "^0.7.3",
        "formidable": "^2.0.1",
        "proxy": "^1.0.2"
    },
    "scripts": {
        "build": "app-bundler && npm run build:dts",
        "build:dts": "tsc -p tsconfig-build.json && atma run tools/build-dts",
        "test": "atma test",
        "watch": "app-bundler --watch",
        "release": "echo \"run atma pub -m \"message\"\""
    },
    "keywords": [
        "dom selenium webdriver"
    ],
    "atma": {
        "plugins": [
            "atma-loader-babel",
            "atma-io-middleware-importer",
            "atma-loader-ts"
        ],
        "settings": {
            "include": {
                "amd": true,
                "extentionDefault": {
                    "js": "ts"
                }
            },
            "io": {
                "extensions": {
                    "ts": [
                        "atma-io-middleware-importer:read",
                        "atma-loader-ts:read"
                    ]
                }
            },
            "atma-loader-babel": {
                "extensions": [
                    "es6"
                ]
            },
            "atma-loader-ts": {
                "extensions": [
                    "test",
                    "ts"
                ],
                "typescript": {
                    "compilerOptions": {
                        "target": "ES2020",
                        "#if TEST": {
                            "module": "AMD"
                        },
                        "#if !TEST": {
                            "module": "CommonJS"
                        }
                    }
                }
            }
        }
    },
    "app-bundler": {
        "file": "src/export.es6",
        "settings": {
            "atma-loader-ts": {
                "typescript": {
                    "compilerOptions": {
                        "module": "CommonJS",
                        "target": "ES2020"
                    }
                }
            }
        },
        "outputMain": "lib/query.js",
        "package": {
            "module": "commonjs",
            "commonjs": {
                "output": "simplified"
            }
        },
        "dynamicDependencies": [
            "alot",
            "appcfg",
            "atma-utils",
            "atma-io",
            "^selenium",
            "^jsdom$",
            "^cheerio$",
            "^node-fetch$",
            "^crypto$",
            "^https?$",
            "^zlib$",
            "^url$",
            "^form-data$",
            "atma-class",
            "https-proxy-agent"
        ],
        "defaultExtensions": {
            "js": "ts"
        },
        "middlewares": {
            "es6": [
                "atma-io-middleware-importer:read",
                "atma-loader-babel:read"
            ],
            "ts": [
                "atma-io-middleware-importer:read",
                "atma-loader-ts:read"
            ]
        }
    },
    "dependencies": {
        "@types/selenium-webdriver": "^4.0.17",
        "alot": "^0.2.96",
        "atma-class": "^1.1.83",
        "atma-io": "^1.2.52",
        "atma-utils": "^0.2.58",
        "cheerio": "^1.0.0-rc.2",
        "encoding": "^0.1.12",
        "form-data": "^3.0.0",
        "jsdom": "^17.0.0",
        "memd": "^0.3.10",
        "node-fetch": "^2.6.7",
        "selenium-webdriver": "^4.1.1"
    }
}
